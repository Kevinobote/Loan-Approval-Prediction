   "source": [
    "#adding of the cibil rating column\n",
    "\n",
    "\n",
    "def cibil_rating(value):\n",
    "    if 300 <= value <= 549:\n",
    "        return 'Poor'\n",
    "    if 550 <= value <= 649:\n",
    "        return 'Average'\n",
    "    if 650 <= value <= 749:\n",
    "        return 'Good'\n",
    "    if 750 <= value <= 900:\n",
    "        return 'Excellent'\n",
    "    else:\n",
    "        return'Error'\n",
    "    \n",
    "df['CB_Rating'] = df['cibil_score'].apply(cibil_rating)\n",
    "\n",
    "df.head(10)"
   ]
  },
