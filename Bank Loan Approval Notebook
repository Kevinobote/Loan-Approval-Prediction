   "source": [
    "def loan_term_range (term):\n",
    "    if 0 <= term <= 7:\n",
    "        return 'Short Term'\n",
    "    if 8 <= term <= 14:\n",
    "        return 'Intermediate'\n",
    "    if 15 <= term <= 20 :\n",
    "        return 'Long Term'\n",
    "    else:\n",
    "        return 'error'\n",
    "    \n",
    "df['Loan Range'] = df['loan_term'].apply(loan_term_range)\n",
    "\n",
    "df.head(10)"
   ]
