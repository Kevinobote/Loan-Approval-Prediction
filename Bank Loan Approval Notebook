   "source": [
    "def loan_rating(amount):\n",
    "    if 100000 <= amount <= 10000000:\n",
    "        return 'Bronze'\n",
    "    if 10000001 <= amount <= 20000000:\n",
    "        return 'Silver'\n",
    "    if 20000001 <= amount <= 30000000:\n",
    "        return 'Gold'\n",
    "    if 30000001 <= amount <= 40000000:\n",
    "        return 'Platinum'\n",
    "    else:\n",
    "        return 'Error'\n",
    "    \n",
    "    \n",
    "df['Loan Rating'] = df['loan_amount'].apply(loan_rating)\n",
    "    \n",
    "df.head(10)"
   ]
  },
